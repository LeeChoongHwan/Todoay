<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/list_item_due_date_todo_finish_bg"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginHorizontal="10dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bg_todo_finish" />

    <TextView
        android:id="@+id/list_item_due_date_todo_finish_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/regular"
        android:text="Todo 이름"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintLeft_toLeftOf="@id/list_item_due_date_todo_finish_bg"
        android:layout_marginLeft="15dp"
        app:layout_constraintTop_toTopOf="@id/list_item_due_date_todo_finish_date"/>

    <TextView
        android:id="@+id/list_item_due_date_todo_finish_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/regular"
        android:text="08/05"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintRight_toRightOf="@id/list_item_due_date_todo_finish_bg"
        app:layout_constraintTop_toTopOf="@id/list_item_due_date_todo_finish_bg"
        app:layout_constraintBottom_toBottomOf="@id/list_item_due_date_todo_finish_bg"
        android:layout_marginRight="15dp"
        />


</androidx.constraintlayout.widget.ConstraintLayout>